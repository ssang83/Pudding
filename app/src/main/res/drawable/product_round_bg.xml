<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <gradient
                android:angle="360"
                android:endColor="#9d57f2"
                android:gradientRadius="360"
                android:startColor="#5873f4"
                android:type="sweep" />
        </shape>
    </item>
    <item android:top="1.5dp" android:bottom="1.5dp" android:right="1.5dp" android:left="1.5dp">
        <shape android:shape="oval">
            <solid android:color="#ffffff"/>
        </shape>
    </item>
</layer-list>